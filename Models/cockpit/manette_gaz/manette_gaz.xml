<?xml version="1.0"?>

<PropertyList>

 <path>manette_gaz.ac</path>
 
 <animation>
 	<type>rotate</type>
	<object-name>manette gaz</object-name>
 <property>controls/engines/engine/throttle</property>
 	<factor>60</factor>
	<axis>
		<x1-m>0.009</x1-m>
		<y1-m>0.036</y1-m>
		<z1-m>-0.035</z1-m>
		<x2-m>-0.009</x2-m>
		<y2-m>-0.054</y2-m>
		<z2-m>-0.018</z2-m>
	</axis>
    </animation>
<animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>manette gaz</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-cycle</command>
        <property>controls/engines/engine/throttle</property>
      </binding>
    </action>
</animation>
</PropertyList>
