<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

 <!-- Flaps and slats interlocks -->

 <logic>
  <name>Flaps, 1st interlock</name>
  <input>
   <and>
    <or>
     <greater-than>
      <property>instrumentation/airspeed-indicator/indicated-speed-knots</property>
      <value>300</value>
     </greater-than>
     <greater-than>
      <property>velocities/mach</property>
      <value>0.75</value>
     </greater-than>
    </or>
    <property>systems/high-lift-devices/flaps/first-interlock-servicable</property>
   </and>
  </input>
  <output>systems/high-lift-devices/flaps/first-interlock-enabled</output>
 </logic>

 <flipflop>
  <name>Flaps, 2nd interlock</name>
  <type>SR</type>
  <s>
   <and>
    <or>
     <greater-than>
      <property>instrumentation/airspeed-indicator/indicated-speed-knots</property>
      <value>335</value>
     </greater-than>
     <greater-than>
      <property>velocities/mach</property>
      <value>0.85</value>
     </greater-than>
    </or>
    <property>systems/high-lift-devices/flaps/second-interlock-servicable</property>
   </and>
  </s>
  <r>
   <and>
    <less-than>
     <property>instrumentation/airspeed-indicator/indicated-speed-knots</property>
     <value>320</value>
    </less-than>
    <less-than>
     <property>velocities/mach</property>
     <value>0.85</value>
    </less-than>
    <property>systems/high-lift-devices/flaps/second-interlock-servicable</property>
   </and>
  </r>
  <output>systems/high-lift-devices/flaps/second-interlock-enabled</output>
  <output>instrumentation/failure-warning-panel/slats-flaps</output>
  <output>instrumentation/master-warning/caution</output>
 </flipflop>

 <logic>
  <name>Flaps, 2nd interlock, set, debug</name>
  <input>
   <and>
    <or>
     <greater-than>
      <property>instrumentation/airspeed-indicator/indicated-speed-knots</property>
      <value>335</value>
     </greater-than>
     <greater-than>
      <property>velocities/mach</property>
      <value>0.85</value>
     </greater-than>
    </or>
    <property>systems/high-lift-devices/flaps/second-interlock-servicable</property>
   </and>
  </input>
  <output>systems/high-lift-devices/flaps/second-interlock-should-enable</output>
 </logic>

 <logic>
  <name>Flaps, 2nd interlock, reset, debug</name>
  <input>
   <and>
    <less-than>
     <property>instrumentation/airspeed-indicator/indicated-speed-knots</property>
     <value>320</value>
    </less-than>
    <less-than>
     <property>velocities/mach</property>
     <value>0.85</value>
    </less-than>
    <property>systems/high-lift-devices/flaps/second-interlock-servicable</property>
   </and>
  </input>
  <output>systems/high-lift-devices/flaps/second-interlock-should-disable</output>
 </logic>

 <logic>
  <name>Slats, 1st interlock</name>
  <input>
   <and>
    <or>
     <greater-than>
      <property>instrumentation/airspeed-indicator/indicated-speed-knots</property>
      <value>440</value>
     </greater-than>
     <greater-than>
      <property>velocities/mach</property>
      <value>0.98</value>
     </greater-than>
    </or>
    <property>systems/high-lift-devices/slats/first-interlock-servicable</property>
   </and>
  </input>
  <output>systems/high-lift-devices/slats/first-interlock-enabled</output>
 </logic>

 <logic>
  <name>Slats, 2nd interlock</name>
  <input>
   <and>
    <or>
     <greater-than>
      <property>instrumentation/airspeed-indicator/indicated-speed-knots</property>
      <value>470</value>
     </greater-than>
     <greater-than>
      <property>velocities/mach</property>
      <value>1.1</value>
     </greater-than>
    </or>
    <property>systems/high-lift-devices/slats/second-interlock-servicable</property>
   </and>
  </input>
  <output>systems/high-lift-devices/slats/second-interlock-enabled</output>
  <output>instrumentation/failure-warning-panel/slats-flaps</output>
  <output>instrumentation/master-warning/caution</output>
 </logic>

</PropertyList>